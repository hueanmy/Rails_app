<% provide :title, t("static_pages.home") %>
<div class="center jumbotron">
  <h1><%= t ".title" %></h1>
  <h2>
    <%= raw t(".into_html",
      tut: link_to(t(".rails_tutorial"), "http://www.railstutorial.org/"))
    %>
  </h2>
  <%= link_to t(".s_up"), "#", class: "btn btn-lg btn-primary" %>
</div>
<%= link_to image_tag("rails.png", alt: t(".logo")),
  "http://rubyonrails.org/"
%>
<%= link_to image_tag("kitten.jpg", alt: t(".kitten")),
  "https://www.pexels.com/search/cat/"
%>
